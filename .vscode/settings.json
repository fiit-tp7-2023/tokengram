{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "never"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "cSpell.words": [
    "FIIT",
    "Josefin",
    "Lato",
    "nuxt",
    "nuxtjs",
    "ofetch",
    "Raleway",
    "Roboto",
    "sidebase",
    "tailwindcss",
    "vueuc",
    "wght"
  ],
  "files.eol": "\n",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "css.lint.unknownAtRules": "ignore"
}
